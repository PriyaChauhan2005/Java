import java.util.Scanner;

class Main {
    static class Sum {
        int sum;

        int add(int a, int b) {
            sum = a + b;
            return sum;
        }
    }

    public static void main(String[] args) {
        int a, b;
        Scanner sc = new Scanner(System.in);
        Sum s1 = new Sum();
        System.out.println("Enter value a:");
        a = sc.nextInt();
        System.out.println("Enter value b:");
        b = sc.nextInt();  // Corrected from sc.newInt()
        System.out.println("Sum of these two numbers is: " + s1.add(a, b));
    }
}
